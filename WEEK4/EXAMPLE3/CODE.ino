void setup() {
 pinMode ( 3 , OUTPUT);
 pinMode ( 4 , OUTPUT);
 pinMode ( 5 , OUTPUT);
 pinMode ( 6 , OUTPUT);
}

void loop() {
 
digitalWrite ( 3 , HIGH );
delay(781);
digitalWrite ( 3 , LOW );
delay(515);
digitalWrite ( 4 , HIGH );
delay(2014);
digitalWrite ( 4 , LOW );
delay(1348);
digitalWrite ( 5 , HIGH );
delay(343);
digitalWrite ( 5 , LOW );
delay(573);
digitalWrite ( 6 , HIGH );
delay(678);
digitalWrite ( 6 , LOW );
delay(1839);


}
